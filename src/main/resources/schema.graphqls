type Book {
    title: String!
    author: Author
}

type Author {
    name: String!
    surname: String!
}

type Query {
    allBooks: [Book]
    getBook(title: String): Book
}

type Mutation {
    createBook(title: String, name: String, surname: String): Book
}

schema {
    query: Query
    mutation: Mutation
}
